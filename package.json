{
  "name": "comply",
  "version": "1.0.0",
  "description": "A comprehensive compliance dashboard with AI-powered document analysis",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "preview": "cd frontend && npm run preview",
    "install-frontend": "cd frontend && npm install",
    "backend": "python main.py",
    "setup": "npm run install-frontend && pip install -r requirements.txt",
    "verify": "python verify-setup.py",
    "clean": "rm -rf frontend/node_modules frontend/dist venv __pycache__ *.log",
    "lint": "cd frontend && npm run lint",
    "test": "cd frontend && npm run test"
  },
  "keywords": [
    "compliance",
    "regulatory",
    "dashboard",
    "ai",
    "document-analysis"
  ],
  "author": "Comply Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/comply.git"
  }
} 